{
  "name": "ai-assassins-mobile",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "sync": "npx cap sync",
    "ios:add": "npx cap add ios",
    "android:add": "npx cap add android",
    "ios:open": "npx cap open ios",
    "android:open": "npx cap open android",
    "copy": "npx cap copy",
    "build:android": "npx cap sync android && cd android && ./gradlew bundleRelease",
    "build:ios": "npx cap sync ios && xcodebuild -workspace ios/App/App.xcworkspace -scheme App -configuration Release -sdk iphoneos -archivePath ios/build/App.xcarchive archive"
  },
  "dependencies": {
    "@capacitor/android": "^7.1.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/core": "^7.1.0",
    "@capacitor/ios": "^7.1.0",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@revenuecat/purchases-capacitor": "^8.8.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.1.0",
    "typescript": "^5.8.2"
  }
}
